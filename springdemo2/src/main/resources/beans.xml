<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="book" class="com.cgi.demo2.Book">
<property name="id" value="1"/>
<property name="title" value="Java"/>
<property name="author" value="Patrick"/>
</bean>

<bean id="book2" class="com.cgi.demo2.Book">
<property name="id" value="2"/>
<property name="title" value="HTML"/>
<property name="author" value="Steve"/>
</bean>


</beans>
